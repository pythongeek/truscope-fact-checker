{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    { "source": "/api/fact-check", "destination": "/api" },
    { "source": "/api/auto-correct", "destination": "/api" },
    { "source": "/api/serp-search", "destination": "/api" },
    { "source": "/api/webz-news-search", "destination": "/api" },
    { "source": "/api/generate-schema", "destination": "/api" },
    { "source": "/api/health-check", "destination": "/api" },
    { "source": "/api/advanced-editor", "destination": "/api" },
    { "source": "/api/blob-analytics", "destination": "/api" },
    { "source": "/api/blob-save-report", "destination": "/api" },
    { "source": "/api/blob-delete-report", "destination": "/api" },
    { "source": "/api/blob-export-bulk", "destination": "/api" },
    { "source": "/api/blob-save-batch-results", "destination": "/api" },
    { "source": "/api/blob-save-fact-database", "destination": "/api" },
    { "source": "/api/blob-load-editor-history", "destination": "/api" }
  ]
}
